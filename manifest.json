{
  "manifest_version": 3,
  "name": "Noest Hub Navbar Customizer",
  "version": "1.0",
  "description": "Changes the navigation container background color on Noest Hub",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": [
    "https://app.noest-dz.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.noest-dz.com/hub*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html"
}
